@model IEnumerable<rcliberty.Web.Models.ApplicationUser>

@{
    ViewBag.Title = "Account/User Management";
}

<h1 class="admin-header">@ViewBag.Title</h1>

<p>
    @Html.ActionLink("Add a New User", "Create", null, new { @class = "btn btn-default admin-create-btn" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
